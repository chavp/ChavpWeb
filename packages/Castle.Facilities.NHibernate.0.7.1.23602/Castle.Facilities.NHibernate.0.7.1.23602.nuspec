<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Castle.Facilities.NHibernate</id>
    <version>0.7.1.23602</version>
    <title>Castle NHibernate Facility</title>
    <authors>Henrik Feldt</authors>
    <owners>Henrik Feldt</owners>
    <licenseUrl>https://github.com/haf/Castle.Facilities.NHibernate/raw/develop/License.txt</licenseUrl>
