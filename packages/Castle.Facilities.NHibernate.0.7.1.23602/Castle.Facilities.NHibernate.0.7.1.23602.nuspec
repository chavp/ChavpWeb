      <dependency id="FluentNHibernate" version="1.3.0.733" />
      <dependency id="NHibernate" version="3.3.1.4000" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Transactions" targetFramework=".NETFramework4.0" />
    </frameworkAssemblies>
    <references>
      <reference file="Castle.Facilities.NHibernate.dll" />
    </references>
  </metadata>
</package>